{
  "name": "test-proj",
  "version": "1.0.0",
  "scripts": {
    "start": "blitz start",
    "studio": "blitz db studio",
    "build": "blitz build",
    "start:production": "blitz start --production --port $PORT",
    "heroku-postbuild": "blitz db migrate && blitz build --production",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "browserslist": [
    "defaults"
  ],
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged && pretty-quick --staged",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@chakra-ui/core": "0.8.0",
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "@exampledev/new.css": "1.1.3",
    "@prisma/cli": "2.10.1",
    "@prisma/client": "2.10.1",
    "@styled-icons/material": "10.18.0",
    "@udecode/slate-plugins": "0.67.4",
    "blitz": "0.24.3",
    "emotion-theming": "10.0.27",
    "final-form": "4.20.1",
    "prismjs": "1.22.0",
    "react": "0.0.0-experimental-7f28234f8",
    "react-dom": "0.0.0-experimental-7f28234f8",
    "react-error-boundary": "2.3.2",
    "react-final-form": "6.5.2",
    "secure-password": "4.0.0",
    "slate": "0.59.0",
    "slate-history": "0.59.0",
    "slate-hyperscript": "0.59.0",
    "slate-react": "0.59.0",
    "styled-components": "5.2.1",
    "use-debounce": "5.0.3",
    "zod": "1.11.10"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.0",
    "@testing-library/react-hooks": "3.4.2",
    "@types/jest": "26.0.15",
    "@types/react": "16.9.55",
    "@types/secure-password": "3.1.0",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-styled-components": "1.11.1",
    "eslint": "7.12.1",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.1",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.0",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "react-test-renderer": "16.14.0",
    "ts-jest": "26.4.3",
    "typescript": "4.0.5"
  },
  "private": true
}
